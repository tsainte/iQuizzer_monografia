@MANUAL{ATA8:ACS,
  TITLE =        {AT Attachment 8 - ATA/ATAPI Command Set (ATA8-ACS))},
  author =       {International Committee for Information Technology Standards (INCITS) T13 Technical Committee},
  organization = {American National Standard for Information Technilogy},
  address =      {},
  edition =      {Projeto T13/1699-D},
  month =        {Maio},
  year =         {2007},
  note =         {},
  abstract =     {This standard specifies the AT Attachment command set between host systems and storage devices. It provides
a common command set for systems manufacturers, system integrators, software suppliers, and suppliers of
intelligent storage devices. It includes the PACKET feature set implemented by devices commonly known as
ATAPI devices. This standard maintains a high degree of compatibility with the AT Attachment Interface with
Packet Interface - 7 (ATA/ATAPI-7) volume 1, INCITS 397-2004, and while providing additional functions, is not
intended to require changes to devices or software that comply with previous T13 standards.},
  keywords =     {},
  source =       {Working Draft},
}

